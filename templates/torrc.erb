###
### File managed by Puppet
###

<% @socksport.each do |socksportarray| -%>
SocksPort <%= socksportarray %>
<% end -%>
<% @sockspolicy.each do |sockspolicyarray| -%>
SocksPolicy <%= sockspolicyarray %>
<% end -%>
<% @orport.each do |orportarray| -%>
ORPort <%= orportarray %>
<% end -%>
<% if @address -%>
Address <%= address %>
<% end -%>
<% if @outboundbindaddress -%>
OutboundBindAddress <%= outboundbindaddress %>
<% end -%>
<% if @nickname -%>
Nickname <%= nickname %>
<% end -%>
BandwidthRate <%= @bandwidthrate %>
BandwidthBurst <%= @bandwidthburst %>
<% if @contactinfo -%>
ContactInfo <%= contactinfo %>
<% end -%>
<% @dirport.each do |dirportarray| -%>
DirPort <%= dirportarray %>
<% end -%>
<% if @dirportfrontpage -%>
DirPortFrontPage <%= dirportfrontpage %>
<% end -%>
<% if @myfamily -%>
MyFamily <%= myfamily %>
<% end -%>
<% @exitpolicy.each do |exitpolicyarray| -%>
ExitPolicy <%= exitpolicyarray %>
<% end -%>
NumCpus <%= @numcpus %>
<% @dnsport.each do |dnsportarray| -%>
DNSPort <%= dnsportarray %>
<% end -%>
<% @transport.each do |transportarray| -%>
TransPort <%= transportarray %>
<% end -%>
<% if @virtualaddrnetwork -%>
VirtualAddrNetworkIPv4 <%= virtualaddrnetwork %>
<% end -%>
<% if @virtualaddrnetwork6 -%>
VirtualAddrNetworkIPv6 <%= virtualaddrnetwork6 %>
<% end -%>
<% if @automaphosts -%>
AutomapHostsOnResolve 1
<% else -%>
AutomapHostsOnResolve 0
<% end -%>
